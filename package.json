{
  "name": "generator-gulp-starter-kit",
  "version": "2.0.6",
  "description": "A simple Gulp workflow that uses sass for css, Webpack, babel.js, image minification, and browsersync. Options available for static html, roots/bedrock app using composer, or install Craft CMS v3.",
  "homepage": "",
  "author": {
    "name": "Tyrel Witcher",
    "email": "tyrel.witcher@gmail.com",
    "url": "https://tyrelwitcher.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "Gulp",
    "webpack",
    "babel.js",
    "sass",
    "yeoman-generator"
  ],
  "devDependencies": {
    "ejs-lint": "^1.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-each": "^0.5.0",
    "husky": "^3.0.9",
    "jest": "^25.5.4",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": 1
    },
    "plugins": [
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "pretest": "gulp lint:ejs",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twitcher07/generator-gulp-starter-kit.git"
  },
  "license": "MIT"
}
