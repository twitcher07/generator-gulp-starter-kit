{
  "require": {
    "craftcms/cms": "^3.5.10",
    "craftcms/mailgun": "^1.4",
    "craftcms/redactor": "2.8.5",
    "dolphiq/redirect": "1.1.0",
    "mmikkel/retcon": "^2.2",
    "nystudio107/craft-seomatic": "3.3.26",
    "putyourlightson/craft-dashboard-begone": "^1.0",
    "sebastianlenz/linkfield": "^1.0",
    "verbb/field-manager": "^2.2",
    "verbb/super-table": "^2.6",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
